{
  "name": "tinp",
  "description": "Tekla Inp language support",
  "author": "CCS",
  "license": "MIT",
  "version": "1.0.0",
  "publisher": "ccs",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "multi-root ready"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "scripts": {
    "build": "tsc --sourceMap",
    "lint": "eslint ."
  },
  "main": "./client/src/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "tinp",
        "aliases": [
          "TINP",
          "tinp"
        ],
        "extensions": [
          ".inp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tinp",
        "scopeName": "source.tinp",
        "path": "./syntaxes/tinp.tmLanguage.json"
      }
    ]
  },
  "test": "mocha",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "^18.14.6",
    "chai": "^5.1.1",
    "eslint": "^9.8.0",
    "globals": "^15.9.0",
    "mocha": "^10.7.0"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-textdocument": "^1.0.4"
  }
}
