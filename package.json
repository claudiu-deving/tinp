{
  "name": "tinp",
  "description": "Tekla Inp language support",
  "author": "CCS",
  "license": "MIT",
  "version": "0.2.1",
  "publisher": "BitLuz",
  "repository": {
    "url": "https://github.com/claudiu-deving/tinp.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "multi-root ready"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "scripts": {
    "build": "tsc --sourceMap",
    "lint": "eslint .",
    "vscode:prepublish": "webpack --mode production --devtool hidden-source-map",
    "webpack": "webpack --config ./webpack.config.js",
    "webpack:dev": "webpack --mode development --watch",
    "test-compile": "webpack --config ./webpack.config.js"
  },
  "main": "./dist/extension.js",
  "activationEvents": [
    "onLanguage:tinp"
  ],
  "contributes": {
    "languages": [
      {
        "id": "tinp",
        "aliases": [
          "TINP",
          "tinp"
        ],
        "extensions": [
          ".inp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tinp",
        "scopeName": "source.tinp",
        "path": "./syntaxes/tinp.tmLanguage.json"
      }
    ]
  },
  "test": "mocha",
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@eslint/js": "^9.8.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "^18.14.6",
    "babel-loader": "^9.2.1",
    "chai": "^5.1.1",
    "eslint": "^9.8.0",
    "globals": "^15.9.0",
    "mocha": "^10.7.0",
    "ts-loader": "^9.5.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-textdocument": "^1.0.4"
  }
}